{
  "name": "openrpg-libs",
  "version": "0.0.10",
  "description": "A set of tools for someone to use to quickly create content for a tabletop RPG world. These are the backend libraries for OpenRPG.",
  "scripts": {
    "preinstall": "npm run release-build",
    "build": "node build.js",
    "rebuild": "node build.js rebuild",
    "release-build": "node build.js rebuild release",
    "clean": "node build.js clean",
    "install": "node-gyp configure build --verbose"
  },
  "gypfile": true,
  "main": "wrappers/export.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incomingstick/OpenRPG.git"
  },
  "author": "Nick Gaulke",
  "license": "OSL",
  "bugs": {
    "url": "https://github.com/incomingstick/OpenRPG/issues"
  },
  "homepage": "https://openrpg.io",
  "dependencies": {},
  "devDependencies": {
    "node-gyp": "^5.0.4"
  }
}
