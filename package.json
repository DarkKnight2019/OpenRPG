{
  "name": "openrpg-libs",
  "version": "0.0.21",
  "description": "A set of tools for someone to use to quickly create content for a tabletop RPG world. These are the backend libraries for OpenRPG.",
  "scripts": {
    "start": "ts-node src/nodejs/export.ts",
    "build": "cmake-js build --debug --verbose",
    "rebuild": "cmake-js rebuild --debug --verbose",
    "clean": "cmake-js clean --verbose",
    "build:core": "node build.js",
    "release:core": "node build.js release",
    "install": "cmake-js build --verbose"
  },
  "main": "src/nodejs/export.ts",
  "types": "src/nodejs/openrpg.d.ts",
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "6.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incomingstick/OpenRPG.git"
  },
  "author": "Nick Gaulke",
  "license": "OSL",
  "bugs": {
    "url": "https://github.com/incomingstick/OpenRPG/issues"
  },
  "homepage": "https://openrpg.io",
  "dependencies": {
    "cmake-js": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.12",
    "node-gyp": "^5.0.4",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
